body {
}

.logo {
	display: none;
}

.content {
	min-height: 1200px;
}


#header {
	position: absolute;
	height: 80px;
	top: 0;
	width: 100%;
	z-index: 101;
}

#header .container {
	position: relative;
}

#header .navigator {
	position: absolute;
	right: 0;
	top: 20px;
	background: rgba(0,0,0,0.3);
	height: 50px;
	border-radius: 3px 0 0 3px;
	box-shadow: 0 0 4px #000;
}

#header .navigator a {
	color: #fff;
	font-size: 16px;
	height: 100%;
	width: 100%;
	display: block;
}

#header .navigator .nav-item {
	float: left;
	position: relative;
	margin-left: 1px;
}

#header .navigator .home {
	margin-left: 0;
}

#header .navigator .nav-item .box {
	height: 50px;
	line-height: 50px;
	width: 120px;
	text-align: center;
}

#header .navigator .home .box {
	border-radius: 3px 0 0 3px;
}

#header .navigator .current .box, #header .navigator .nav-item:hover .box {
	background: rgba(0,0,0,0.5);
}

#header .navigator .nav-item .sub {
	display: none;
	position: absolute;
	top: 50px;
	left: 0;
	width: 100%;
	background: rgba(0,0,0,0.5);
}

#header .navigator .nav-item .sub li {
	line-height: 40px;
	text-align: center;
}

#header .navigator .nav-item .sub a {
	font-size: 15px;
}

#header .navigator .nav-item.hover .sub, #header .navigator .nav-item:hover .sub {
	display: block;
}

#footer {
	clear: both;
	margin-top: 200px;
	background-color: #686c6f;
	font-size: 13px;
}

#footer .container {
	overflow: hidden;
	height: 350px;
	position: relative;
}

#footer a {
	color: #eee;
}

#footer .fnav {
	display: flex;
	vertical-align: top;
	padding-top: 50px;
}

#footer .container .nav-item {
	flex: 1 1 auto;
	width: 140px;
	display: inline-block;
	text-align: center;
	line-height: 30px;
}

#footer .container .nav-item .box a {
	font-weight: 600;
	color: #fff;
	line-height: 36px;
}

#footer .cert {
	color: #ccc;
	text-align: center;
	width: 100%;
	position: absolute;
	bottom: 10px;
	left: 10px;
}